<%= turbo_frame_tag "quote_#{quote.id}" do %>
  <div class="quote">
    <div class="quote_actions d-flex mt-3">
      <%= link_to quote.name, quote_path(quote), data: { turbo_frame: "_top" }, class: "mr-5" %>
      <%= button_to "Delete", quote_path(quote), method: :delete, class: "btn btn-danger mr-2" %>
      <%= link_to "Edit", edit_quote_path(quote), class: "btn btn-secondary" %>
    </div>
    </div>
  </div>
<% end %>